<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgpapers"
    tools:context=".activities.branch_paper">

    <Button
        android:id="@+id/button2"
        android:layout_width="46dp"
        android:layout_height="38dp"
        android:layout_marginStart="312dp"
        android:layout_marginLeft="312dp"
        android:layout_marginTop="16dp"
        android:background="@android:drawable/ic_menu_upload"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.381" />

    <Button
        android:id="@+id/button3"
        android:layout_width="46dp"
        android:layout_height="38dp"
        android:layout_marginTop="40dp"
        android:background="?attr/homeAsUpIndicator"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="206dp"
        android:layout_height="40dp"
        android:text="PAST PAPERS"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.532" />

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="350dp"
        android:layout_height="517dp"
        android:layout_margin="20dp"
        app:columnCount="2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.878"
        app:rowCount="3">

        <ImageView
            android:id="@+id/civil_img"
            android:layout_width="151dp"
            android:layout_height="149dp"
            android:layout_margin="10dp"
            android:background="@drawable/bg"
            app:layout_column="0"
            app:layout_row="1" />

        <ImageView
            android:id="@+id/computer_img"
            android:layout_width="150dp"
            android:layout_height="149dp"
            android:layout_margin="10dp"
            android:background="@drawable/bg"
            app:layout_column="0"
            app:layout_row="0" />

        <ImageView
            android:id="@+id/it_img"
            android:layout_width="150dp"
            android:layout_height="152dp"
            android:layout_margin="10dp"
            android:background="@drawable/bg"
            app:layout_column="1"
            app:layout_row="0" />

        <ImageView
            android:id="@+id/electrical_img"
            android:layout_width="150dp"
            android:layout_height="149dp"
            android:layout_margin="10dp"
            android:background="@drawable/bg"
            app:layout_column="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_row="1" />

        <ImageView
            android:id="@+id/mechanical_img"
            android:layout_width="150dp"
            android:layout_height="149dp"
            android:layout_margin="10dp"
            android:background="@drawable/bg"
            app:layout_column="0"
            app:layout_row="2" />

        <ImageView
            android:id="@+id/ece_img"
            android:layout_width="150dp"
            android:layout_height="149dp"
            android:layout_margin="10dp"

            android:background="@drawable/bg"
            app:layout_column="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_row="2" />

        <TextView
            android:id="@+id/computer"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_margin="40dp"
            android:layout_marginTop="70dp"

            android:text="Computer "
            android:textSize="20dp"
            app:layout_column="0"
            app:layout_row="0" />

        <TextView
            android:id="@+id/it"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_margin="60dp"
            android:layout_marginTop="70dp"

            android:text=" I.T."
            android:textSize="20dp"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/civil"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_margin="40dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="70dp"

            android:text="   Civil "
            android:textSize="20dp"
            app:layout_column="0"
            app:layout_row="1" />

        <TextView
            android:id="@+id/electrical"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_margin="40dp"
            android:layout_marginTop="80dp"

            android:text="  Electrical"
            android:textSize="20dp"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/mechanical"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_margin="4dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="70dp"

            android:text="Mechanical"
            android:textSize="20dp"
            app:layout_column="0"
            app:layout_row="2" />

        <TextView
            android:id="@+id/ece"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_margin="40dp"
            android:layout_marginTop="70dp"

            android:text="     E.C.E."
            android:textSize="20dp"
            app:layout_column="1"
            app:layout_row="2" />

    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>